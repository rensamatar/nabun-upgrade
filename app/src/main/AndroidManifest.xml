<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nabun_upgrade.nabun" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.nabun_upgrade.config.Application"
        android:allowBackup="true"
        android:icon="@drawable/lib_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CareerViewActivity"
                  android:label="Nabun" />
        <activity android:name=".EventViewActivity"
                  android:label="Nabun" />
        <activity android:name="com.nabun_upgrade.utility.CustomSelectListView"
                  android:label="Nabun" />
        <activity android:name=".PhotoViewActivity"
                  android:label="Nabun" />
    </application>

</manifest>
